#include<stdio.h>
#include<stdint.h>

struct Dataset
{
char data1;
int data2;
char data3;
short data4;



};

int main(){

	struct Dataset data;
	data.data1 =0x11;
	data.data2=0xffffeeee;

	data.data3=0x22;
	data.data4=0xabcd;

	printf("size of the struct variable  %u \n",sizeof(data));
	return 0;



}

